{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(js|vue)$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "verbose": true
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs"
  ]
}